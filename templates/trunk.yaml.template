# Template for .trunk/trunk.yaml in consuming projects
# Copy this to your project's .trunk/trunk.yaml and adjust as needed

version: 0.1
cli:
  version: 1.22.8
plugins:
  sources:
    - id: trunk
      ref: v1.6.5
      uri: https://github.com/trunk-io/plugins
    - id: pke-config
      local: /home/pke/Projects/trunk

# Uncomment to add project-specific linters or overrides:
