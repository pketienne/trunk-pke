{
	"$schema": "https://dprint.dev/schemas/v0.json",
	"lineWidth": 80,
	"newLineKind": "lf",
	"markdown": {
		"textWrap": "maintain",
		"emphasisKind": "underscores",
		"strongKind": "asterisks"
	},
	"excludes": ["**/node_modules", "**/.git", "**/dist", "**/build", "**/target"],
	"plugins": [
		"https://github.com/pketienne/dprint-plugin-markdown/releases/download/v0.20.0-pke.1/dprint_plugin_markdown.wasm"
	]
}
